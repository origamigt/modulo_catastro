<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" version="3.1"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>OmegaConfigs</ejb-name>
            <env-entry>
                <env-entry-name>com.origami.sgm.services.ejbs.censocat.OmegaConfigs/dburl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <!--<env-entry-value>jdbc:postgresql://186.101.220.187:5432/fotos_mocache</env-entry-value>-->
                <env-entry-value>jdbc:postgresql://192.168.100.3:5432/fotos_mocache</env-entry-value>
                <!--<env-entry-value>jdbc:postgresql://localhost:5432/fotos_mocache</env-entry-value>-->
            </env-entry>
            <env-entry>
                <env-entry-name>com.origami.sgm.services.ejbs.censocat.OmegaConfigs/dbuser</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>sisapp</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>com.origami.sgm.services.ejbs.censocat.OmegaConfigs/dbpass</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>sisapp98</env-entry-value>
                <!--<env-entry-value>Fg22clIeo4M</env-entry-value>-->
            </env-entry>
            <env-entry>
                <env-entry-name>com.origami.sgm.services.ejbs.censocat.OmegaConfigs/tableName</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>archivos.doc_file</env-entry-value>
            </env-entry>
        </session>

        <session>
            <ejb-name>GeodataService</ejb-name>
            <env-entry>
                <env-entry-name>com.origami.sgm.geo.GeodataService/geoserverUrl</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <!--<env-entry-value>http://catastro.ibarra.gob.ec/geoserver/</env-entry-value>-->
                <!--<env-entry-value>http://186.101.220.187:8088/geoserver/</env-entry-value>-->
                <env-entry-value>http://192.168.100.2/geoserver/</env-entry-value>
                <!-- <env-entry-value>http://localhost:8098/geoserver</env-entry-value> -->
            </env-entry>
        </session>

    </enterprise-beans>

</ejb-jar>
